#include<stdio.h>
/*
*Time：2019.12.8
*Fuction：数组指针
*
**/
int main()
{
    int a=3;
    int b=6;
    int c=8;
    int d=12;
    int e=20;
    int* arr[5]={&a,&b,&c,&d,&e};
    int** p = arr;
    
    //p == arr
    printf("%p %p\n",p,arr);
    
    //*p == *arr
    printf("%p %p\n",*p,*arr);
    
    //p+1 == arr+1
    printf("%p %p\n",p+1,arr+1);
    
    //*(p+1) == *(arr+1) ==arr[1] 取指向指针数组中的第二个元素的地址 (取出来的是一个十六进制的地址)
    printf("%p %p\n",*(p+1),*(arr+1))
    
    //**(p+1)  == **(arr+1) ==*arr[1] 取指向arr的指针数组的第二个元素的值 (取出来的是&b中的值6)
    printf("%d %d\n",**(p+1),**(arr+1));
    
    puts("************************************************************\n");
    
    //*p+1 == *arr+1 取的是指针的下一个指针地址
    printf("%p %p\n",*p+1,*arr+1);
    
    //*(*p+1) == *(*arr+1)
    //变量a的下面4个字节
    printf("%d %d\n",*(*p+1),*(*arr+1));
    return 0;
}
    
    
    
